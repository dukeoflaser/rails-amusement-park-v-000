<h1>Attractions</h1>
<ul>
  <% @attractions.each do |attraction| %>
    <li>
      <%= link_to "Go on #{attraction.name}", attraction_path(attraction) unless current_user.admin %>
      <%= link_to "Show #{attraction.name}", attraction_path(attraction) if current_user.admin %>
    </li>
  <% end %>
</ul>
<%= link_to 'New Attraction', new_attraction_path if current_user.admin %>
